<%- include ('partials/header')%>

    <h1 class="hello">Albums</h1>
    <section>
        <% albumResults.forEach(album => { %>
            <article>
            <img src=<%=album.images != '' ? album.images[0].url :'assets/img/empty.png' %> alt="">
            <h2><%=album.name%></h2>
            <a href="/albums/<%=album.id%>">Show me Songs</a>
            </article>
        <% }) %>
        </section>
    <%- include ('partials/footer')%>